package loops;

public class WhileLoop {

	public static void main(String[] args) {
		/*
		 * While ejecuta un bloque de c贸digo hasta que se cumpla una condici贸n dada
		 * 
		 * While (expresi贸n) {
		 * bloque de c贸digo a ejecutar
		 * (contador-iterador)
		 * }
		 * 
		  */
		
		int cuenta =0;
		while (cuenta < 2) {
			cuenta ++;
			System.out.println("La cuenta es de "+ cuenta);
		}

	}

}
